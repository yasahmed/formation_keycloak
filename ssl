      openssl req -newkey rsa:2048 -nodes  -keyout keycloak-server.key.pem -x509 -days 3650 -out keycloak-server.crt.pem 

      /opt/keycloak/bin/kc.sh start-dev  --http-port=8080 --https-port=443 --https-certificate-file=/opt/keycloak/ssl/keycloak-server.crt.pem --https-certificate-key-file=/opt/keycloak/ssl/keycloak-server.key.pem
